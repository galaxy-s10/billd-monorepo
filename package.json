{
  "name": "billd-use",
  "version": "0.0.1",
  "description": "billd's utils",
  "keywords": [
    "billd",
    "utils",
    "javascript",
    "css"
  ],
  "homepage": "http://project.hsslive.cn/billd-use/",
  "bugs": {
    "url": "https://github.com/galaxy-s10/billd-use/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/galaxy-s10/billd-use"
  },
  "license": "ISC",
  "author": "galaxy-s10 <https://github.com/galaxy-s10>",
  "scripts": {
    "build": "ts-node ./scripts/build.ts",
    "build:rollup": "rollup -c rollup.config.ts",
    "clean": "rimraf packages/*/dist",
    "cz": "cz",
    "doc": "typedoc --options ./typedoc.config.json",
    "lint": "eslint --config ./.eslintrc.js . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --config ./.eslintrc.js . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "publish": "ts-node ./scripts/publish.ts",
    "release": "standard-version",
    "update": "ts-node ./scripts/update.ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.18.9",
    "@babel/runtime": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "rollup": "^2.77.0",
    "rollup-plugin-typescript2": "^0.32.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "chalk": "^4",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.26.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.8",
    "typescript": "^4.7.4"
  }
}
